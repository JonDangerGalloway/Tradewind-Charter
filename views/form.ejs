<%- include("header"); %> 

    <h1 class="charter-estimate">Charter Estimate</h1>

    
    <div class="mb-3 row estimate-form">

        <form action="/estimate" method="post">
            <input class="form-control form-control-lg form-input" type="text" name="leavingFrom" placeholder="Leaving From" autocomplete="off">
            <input class="form-control form-control-lg form-input" type="text" name="goingTo" placeholder="Going To" autocomplete="off">
            <input class="form-control form-control-lg form-input" type="number" min="1" max="15" name="passengers" placeholder="Passengers" aria-autocomplete="off">
            <button type="submit" class="btn btn-dark form-btn">Estimate My Flight</button>
        </div>
            
        </form>
    </div>

    <h2> <%= ejsTitle %> </h2>

    
<%- include("footer"); %>  